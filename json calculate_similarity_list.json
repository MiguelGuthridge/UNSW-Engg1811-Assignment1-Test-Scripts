[ 
    {
        "test_type": "Provided 1",
        "data_series": [-1, 2, -2, 3, 41, 38, 22, 10, -1, 3],
        "pattern": [40, 30, 20, 10],
        "expected_output": [10, 490, 1210, 2330, 3320, 2370, 1190]
    },

    {
        "test_type": "Provided 2",
        "data_series": [-1, 2, 21, 15, -30.34, 38, 22.2, 4],
