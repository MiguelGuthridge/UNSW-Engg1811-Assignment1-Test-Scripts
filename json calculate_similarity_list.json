        "expected_output": [56, 34, -1.02, 219.34, -123.1, 179.8]
    },

    {
        "test_type": "Insufficient data",
        "data_series": [-1, 2, -2, 5, 12],
        "pattern": [40, 30, 20, -10, 15, 34, 13],
        "expected_output": "Insufficient data"
    },

    {
        "test_type": "Empty pattern",
        "data_series": [-1, 2, -2, 5, 12],
        "pattern": [],
        "expected_output": "Insufficient data"
    },

    {
        "test_type": "Pattern length = 1",
        "data_series": [-1, 2, -2, 5, 12],
        "pattern": [1],
        "expected_output": [-1, 2, -2, 5, 12]
    }
